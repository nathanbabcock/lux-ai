{
  "name": "lux-ai-bot",
  "version": "0.0.3",
  "description": "WIP bot entry in 2021 Lux AI competition",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && npm run sim && npm run vis",
    "build": "tsc",
    "watch": "tsc -w",
    "sim": "lux-ai-2021 --storeLogs=false --statefulReplay dist/agents/tree-search.js dist/agents/hello-world.js --out=replays/replay.json",
    "vis": "lux-ai-vis replays/replay.json",
    "sim-vis": "npm run sim && npm run vis",
    "submission": "tar -czvf submissions/director-expander.tar.gz dist main.py"
  },
  "author": "Nathan Babcock",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/node": "^16.10.1",
    "concurrently": "^6.2.1",
    "lux-ai-vis": "^3.1.3",
    "typescript": "^4.4.3",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@lux-ai/2021-challenge": "^3.1.0",
    "dimensions-ai": "^5.1.3"
  }
}
