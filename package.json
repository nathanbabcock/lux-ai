    "vis": "lux-ai-vis replays/replay.json",
    "sim-vis": "npm run sim && npm run vis"
  },
  "author": "Nathan Babcock",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/node": "^16.10.1",
    "concurrently": "^6.2.1",
    "lux-ai-vis": "^3.1.3",
    "typescript": "^4.4.3",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@lux-ai/2021-challenge": "^3.1.0"
  }
}
