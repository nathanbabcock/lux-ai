{
  "name": "lux-ai-bot",
  "version": "0.0.3",
  "description": "WIP bot entry in 2021 Lux AI competition",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && npm run sim && npm run vis",
    "test": "jest --watch",
    "path-test": "node dist/scripts/path-test.js",
    "build": "tsc",
    "watch": "tsc -w",
    "w": "npm run watch",
    "sim": "lux-ai-2021 --storeLogs=false --statefulReplay --out=replays/replay.json dist/agents/rule-based.js dist/agents/hello-world.js",
    "vis": "lux-ai-vis replays/replay.json",
    "sim-vis": "npm run sim && npm run vis",
    "tournament": "lux-ai-2021 --storeLogs=false --storeReplay=false --rankSystem=wins --tournament dist/agents/tree-search.js dist/agents/director-expander.js",
    "webpack": "webpack --config ./webpack.config.js",
    "submission": "npm run webpack && tar -czvf submissions/dfs-first-city.tar.gz dist main.py"
  },
  "author": "Nathan Babcock",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "concurrently": "^6.2.1",
    "jest": "^27.2.4",
    "lux-ai-vis": "^3.1.3",
    "node-loader": "^2.0.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "wait-on": "^6.0.0",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@lux-ai/2021-challenge": "^3.1.0",
    "dimensions-ai": "^5.1.3",
    "reflect-metadata": "^0.1.13",
    "class-transformer": "^0.4.0"
  }
}
